{
  "name": "xbo-nextjs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "build:analyze": "ANALYZE=true npm run build",
    "apollo:download-schema": "apollo client:download-schema schema.graphql",
    "apollo:generate-types": "rm -rf __generated__ && apollo client:codegen --target typescript --outputFlat",
    "apollo:sync": "npm run apollo:download-schema && npm run apollo:generate-types",
    "lint": "eslint '**/*.{js,jsx,ts,tsx}'",
    "prettier": "prettier --write '**/*.{js,jsx,ts,tsx}'",
    "test": "jest -c test/jest.config.js",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run --config video=false",
    "cypress:test": "start-server-and-test start http://localhost:3000 cypress:run"
  },
  "dependencies": {
    "@apollo/client": "^3.4.1",
    "@next/bundle-analyzer": "^10.0.5",
    "@reduxjs/toolkit": "^1.6.1",
    "axios": "^0.21.1",
    "graphql": "^15.4",
    "graphql-request": "^3.5.0",
    "next": "10.2.0",
    "next-compose-plugins": "^2.2.1",
    "prettier": "^2.3.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-query": "^3.19.1",
    "react-redux": "^7.2.4"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@storybook/addon-actions": "^6.3.6",
    "@storybook/addon-essentials": "^6.3.6",
    "@storybook/addon-knobs": "^6.3.0",
    "@storybook/addon-links": "^6.3.6",
    "@storybook/preset-scss": "^1.0.3",
    "@storybook/react": "^6.3.6",
    "@testing-library/react": "^12.0.0",
    "@testing-library/react-hooks": "^5.0.0",
    "@types/jest": "^26.0.24",
    "@types/node": "^16.4.8",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-redux": "^7.1.16",
    "@typescript-eslint/eslint-plugin": "^4.9.1",
    "@typescript-eslint/parser": "^4.9.1",
    "apollo": "^2.32.1",
    "autoprefixer": "^10.1.0",
    "babel-loader": "^8.2.2",
    "css-loader": "^5.0.1",
    "cypress": "^8.1.0",
    "dotenv": "^8.2.0",
    "eslint": "^7.15.0",
    "eslint-config-prettier": "^7.0.0",
    "eslint-plugin-prettier": "^3.2.0",
    "identity-obj-proxy": "^3.0.0",
    "isomorphic-unfetch": "^3.1.0",
    "jest": "^26.0",
    "nock": "^13.0.6",
    "postcss": "^8.3.6",
    "start-server-and-test": "^1.11.0",
    "style-loader": "^2.x",
    "tailwindcss": "^2.2.7",
    "ts-jest": "^26.0",
    "typescript": "^4.3.5"
  }
}
